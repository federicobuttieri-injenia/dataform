config {
    type: "view",
    schema: "DM_CRM",
    name: "f_sms_result_output_partitions"
}

SELECT DISTINCT DATE(a.dataora_creazione) AS dataora_creazione AS value
FROM ${ref("S_LOG_SMS_RESULT")}, UNNEST (attributes) a
WHERE _PARTITIONDATE IN (SELECT * FROM ${ref("f_sms_result_input_partitions")})
		