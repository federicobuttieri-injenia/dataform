config {
    type: "view",
    schema: "injenia",
    name: "f_email_result_output_partitions"
}

SELECT DISTINCT DATE(a.dataora_creazione) AS dataora_creazione AS value
FROM `mmfg-dwh-gruppo-prod.DM_SELLOUT_STAGING.S_LOG_EMAIL_RESULT`, UNNEST (attributes) a
WHERE _PARTITIONDATE IN (SELECT * FROM ${ref("f_email_result_input_partitions")})
		