config {
    type: "view",
    schema: "injenia",
    name: "s_prezzi_ext_from_ricarichi_input_output_partitions"
}

SELECT DISTINCT  PARSE_DATE("%Y%m%d", data_creazione) AS data_creazione
FROM `mmfg-dwh-gruppo-prod.DM_PREZZO_LISTINO.D_RICARICHI`
WHERE data_creazione IS NOT NULL 
AND data_creazione != ''
AND DATE(CAST(modificato AS TIMESTAMP)) >= CURRENT_DATE
		