config {
    type: "view",
    schema: "injenia",
    name: "s_email_result_output_partitions"
}

SELECT DISTINCT DATE(a.dataora_creazione) AS dataora_creazione AS value
FROM ${ref("S_LOG_EMAIL_RESULT")}, UNNEST (attributes) a
WHERE _PARTITIONDATE IN (SELECT * FROM ${ref("s_email_result_input_partitions")})
		