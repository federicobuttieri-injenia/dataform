config {
    type: "table",
    schema: "injenia",
    name: "D_MODELLO_PERMANENTE"
}

SELECT * EXCEPT (action)
FROM (
	SELECT
		id,
		attributes.*,
		timestamp AS ts_inserimento,
	FROM ${ref("S_MODELLO_PERMANENTE")}
	WHERE True
	QUALIFY ROW_NUMBER() OVER(PARTITION BY id ORDER BY timestamp DESC) = 1
)
WHERE action != 'DELETE' 
		