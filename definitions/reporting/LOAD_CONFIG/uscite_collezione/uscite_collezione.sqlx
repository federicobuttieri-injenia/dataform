config {
    type: "table",
    schema: "injenia",
    name: "D_USCITA_COLLEZIONE"
}

SELECT * EXCEPT (action)
FROM (
	SELECT
		id,
		attributes.*,
		timestamp AS ts_inserimento,
	FROM ${ref("DM_SELLOUT_STAGING", "S_USCITA_COLLEZIONE")}
	WHERE True
	QUALIFY ROW_NUMBER() OVER(PARTITION BY id ORDER BY timestamp DESC) = 1
)
WHERE action != 'DELETE' 
		