config {
    type: "table",
    schema: "injenia",
    name: "D_RAGGR_GRANCLASSE"
}

SELECT * EXCEPT (action)
FROM (
	SELECT
		id,
		attributes.*,
		timestamp AS ts_inserimento,
	FROM `mmfg-dwh-gruppo-prod.DM_SELLOUT_STAGING.S_RAGGR_GRANCLASSE`
	WHERE True
	QUALIFY ROW_NUMBER() OVER(PARTITION BY id ORDER BY timestamp DESC) = 1
)
WHERE action != 'DELETE' 
		